{
  "name": "sumantablog/dragndroplaravelmenu",
  "description": "Drag And Drop Menu Builder in Laravel",
  "keywords": ["Laravel Menu", "Drag & Drop Menu", "Menu Builder"],
  "authors": [{
      "name": "Sumanta Kabiraj",
      "email": "sumantablog@gmail.com",
      "role": "Developer"
    },
    {
      "name": "Arpan Ghosh",
      "email": "ghosharpan767@gmail.com",
      "role": "Developer"
    }
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.1"
  },
  "require-dev": {
    "php": ">=7.2",
    "illuminate/support": "5.8.* || ^6.0"
  },
  "autoload": {
    "psr-4": {
      "Sumantablog\\Menu\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Sumantablog\\Menu\\MenuServiceProvider"
      ],
      "aliases": {
        "Menu": "Sumantablog\\Menu\\Facades\\Menu"
      }
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}